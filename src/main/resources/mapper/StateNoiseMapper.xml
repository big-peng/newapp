<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hzaihua.jfoenix.dao.StateNoiseDao">
    <select id="queryByDeviceCode" resultType="com.hzaihua.jfoenix.entity.StateNoise">
        select * from ${deviceTableName} where deviceCode=#{deviceCode}
    </select>
    <!-- 删除 -->
    <delete id="deleteStateNoise">
        delete from ${deviceTableName} where deviceCode=#{deviceCode}
    </delete>
</mapper>
